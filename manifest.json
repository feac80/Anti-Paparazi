{
  "name": "Anti paparazi",
  "version": "2.0",
  "manifest_version": 2,

  "description": "This extension will remove all the picture from  the current website",
  "content_scripts": [
    {
      "matches": ["https://*/*", "http://*/*"],
      "js": ["content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },

  "permissions": ["background", "http://*/*", "https://*/*", "storage"]
}
